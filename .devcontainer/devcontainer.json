{
	"name": "Development Docker container",
	"build": { 
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"containerEnv": {
		"APIFY_TOKEN": "${localEnv:APIFY_TOKEN}"
	},
	"customizations": {
		"vscode": {
			"settings": {},
			"extensions": [
				"streetsidesoftware.code-spell-checker",
				"jebbs.plantuml",
				"esbenp.prettier-vscode",
				"ms-python.python"
			]
		}
	}
}
